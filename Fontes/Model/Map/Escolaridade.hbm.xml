<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false" namespace="Model.Entidade" assembly="Model">
  <class name="Model.Entidade.Escolaridade" table="escolaridade">
    <id name="Codigo">
      <column name="ESC_COD" sql-type="int" not-null="true" />
      <generator class="increment" />
    </id>
    <property name="Descricao">
      <column name="ESC_DESCRICAO" sql-type="varchar(40)" not-null="true" />
    </property>

  </class>
</hibernate-mapping>

