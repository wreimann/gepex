<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false" namespace="Model.Entidade" assembly="Model">
  <class name="Model.Entidade.CorRaca" table="cor_raca">
    <id name="Codigo">
      <column name="CRA_COD" sql-type="int" not-null="true" />
      <generator class="increment" />
    </id>
    <property name="Descricao">
      <column name="CRA_DESCRICAO" sql-type="varchar(30)" not-null="true" />
    </property>
   
  </class>
</hibernate-mapping>

