<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false" namespace="Model.Entidade" assembly="Model">
  <class name="Model.Entidade.TipoDocumento" table="tipo_documento">
    <id name="Codigo">
      <column name="TDO_COD" sql-type="int" not-null="true" />
      <generator class="increment" />
    </id>
    <property name="Descricao">
      <column name="TDO_DESCRICAO" sql-type="varchar(30)" not-null="true" />
    </property>
    <property name="Mascara">
      <column name="TDO_MASCARA" sql-type="varchar(30)" />
    </property>
    <property name="Situacao">
      <column name="TDO_SITUACAO" sql-type="tinyint(1)" not-null="true" />
    </property>
  </class>
</hibernate-mapping>

