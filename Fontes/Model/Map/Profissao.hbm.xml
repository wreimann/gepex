<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false" namespace="Model.Entidade" assembly="Model">
  <class name="Model.Entidade.Profissao,Model" table="profissao" lazy="false">
    <id name="Codigo">
      <column name="PRO_COD" sql-type="int" not-null="true"/>
      <generator class="increment" />
    </id>
    <property name="Descricao" column="PRO_DESCRICAO" type="string" not-null="true" />
    <property name="Situacao" column="PRO_SITUACAO" type="bool" not-null="true" />
    <property name="Tipo" column="PRO_TIPO" type="string" not-null="false" />
  </class>
</hibernate-mapping>